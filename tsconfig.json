{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "types": [
            "jest",
            "node",
            "cypress"
          ],
        "resolveJsonModule": true,
        "moduleResolution": "node",
        "baseUrl": "./",
        "paths": {
            "@components/*": [
                "components/*"
            ],
        },
        "module": "esnext",
        "jsx": "react-jsx",
    },
    "include": [
        "components",
    ],
    "exclude": [
        "node_modules",
    ]
}
